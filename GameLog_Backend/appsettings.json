{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Jwt": {
        "Key": "{JWT_SECRET}",
        "Issuer": "GameLogAPI",
        "Audience": "GameLogClient",
        "ExpireHours": 24
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Server={DB_SERVER};Database={DB_NAME};User ID={DB_USER};Password={DB_PASSWORD};TrustServerCertificate=True;"
    }
}